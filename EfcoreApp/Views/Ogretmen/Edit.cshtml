@model Ogretmen


<h2>Öğretmen Günçelleme</h2>
<hr />


<div class="row">
    <div class="col-8">
        <form method="post" asp-controller="Ogretmen" asp-action="Edit">
            <input type="hidden" asp-for="OgretmenId" />
            <div class="mb-3">
                <label asp-for="Ad" class="form-label">Adı</label>
                <input asp-for="Ad" class="form-control" />

            </div>
            <div class="mb-3">
                <label asp-for="Soyad" class="form-label">Soyad</label>
                <input asp-for="Soyad" class="form-control" />

            </div>
            <div class="mb-3">
                <label asp-for="Eposta" class="form-label">Eposta</label>
                <input asp-for="Eposta" class="form-control" />

            </div>
            <div class="mb-3">
                <label asp-for="Telefon" class="form-label">Telefon</label>
                <input asp-for="Telefon" class="form-control" />

            </div>
                <div class="mb-3">
                <label asp-for="BaslamaTarihi" class="form-label">Başlama Tarihi</label>
                <input asp-for="BaslamaTarihi" class="form-control" />

            </div>
            <button class="btn btn-secon dary" type="submit">Kaydet</button>
        </form>
    </div>

</div>
